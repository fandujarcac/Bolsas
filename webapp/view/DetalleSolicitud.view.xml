<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="es.cac.bolsa.BolsaTrabajoPublicada0920.controller.DetalleSolicitud">
	<Page xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" title="Detalle Solicitud" id="pageDet"
		showNavButton="true" navButtonPress="onNavBack" cd:w5g.dt.context="/ZSolicitantePublicadoSet">
		<customHeader>
			<Bar>
				<contentLeft>
					<Button xmlns="sap.m" id="button0" icon="sap-icon://nav-back" press="onNavBack"/>
					<Image src="https://www.cac.es/.resources/cac-module/img/cac_logo_2018.png" height="100px" id="image2" width="auto"/>
				</contentLeft>
				<contentMiddle>
					<Label text="Bolsa de Trabajo {ModelSolicitante>/DescripcionPublic}" design="Bold" id="idLabelBar"/>
				</contentMiddle>
				<contentRight>
					<Label text="Publicado: {path: 'ModelSolicitante>/FechaPublic', type: 'sap.ui.model.type.Date', formatOptions: {style: 'medium'}}" design="Bold" id="idLabelRight"/>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="form0">
				<sap.ui.layout.form:layout>
					<sap.ui.layout.form:ResponsiveGridLayout id="layout0"/>
				</sap.ui.layout.form:layout>
			</sap.ui.layout.form:Form>
		</content>
	</Page>
</mvc:View>